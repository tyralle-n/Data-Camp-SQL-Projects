SELECT public.sales.product_line, TO_CHAR(date, 'Month') AS month,
	public.sales.warehouse, SUM(total) - SUM(payment_fee) AS net_revenue
FROM public.sales
	WHERE client_type = 'Wholesale'

	GROUP BY 
    product_line,
    month,
    warehouse
	ORDER BY 
    product_line,
    month,
    net_revenue DESC;
